{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "max-len": "off",
    "quotes": [
      "error",
      "single"
    ],
    "object-curly-spacing": [
      "error",
      "never"
    ],
    "quote-props": "off",
    "block-spacing": "off",
    "space-before-function-paren": "off",
    "complexity": [
      "warn",
      10
    ],
    "require-await": "error"
  }
}